default: &default
  adapter: mysql2
  encoding: utf8
  username: root
  password: root
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>
  timeout: 5000

development:
  <<: *default
  database: Football_development
test:
  <<: *default
  database: Football_test

production:
  <<: *default
  database: Football_production
